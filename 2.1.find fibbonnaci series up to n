#include<stdio.h>
int main()
{
	printf("Senthamizhselvan\n192321143\n");
	int n,x=0,y=1,sum=1,temp;
	printf("Enter number for fibbonacci:\n");
	scanf("%d",&n);
	printf("%d %d ",x,y);
	for(int i=0;i<n-2;i++)
	{
		temp=x+y;
		printf("%d ",temp);
		sum+=temp;
		x++;
		y++;
	}
printf("\nSum of %d fibbonacci terms is: %d",n,sum);	
}
